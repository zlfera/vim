<script type="text/javascript">
      $(document).ready(function() { //resetpasswd
             $("Button#resetpasswd").click(function() {
                    if (confirm("是否重置密码?")){
                             $.get("api.php", {
                                cmd: "resetpasswd"
                            }, function(a) {
                                $("#passwd").val(a);
                                layer.alert("您的新密码:"+a);
                            });
                              }else{
                        return false;
                        }
                        });        
                    });
    $(document).ready(function() {
        
            $("Button#gift500mb").click(function() {
                $.get("api.php", {
                    cmd: "gift500mb"
                }, function(a) {
                    layer.alert(a);
                     
                });
            });
        });

    </script>
