  <script>
            $(function() {
                var youhui = 0.8;
              $( "#slider-range-min" ).slider({
                range: "min",
                value: 100,
                min: 100,
                max: 1000,
                slide: function( event, ui ) {
                  $( "#amount" ).val( ui.value);
                  $( "#amounts" ).val( ui.value*youhui);//8折
                }
              });
              $( "#amount" ).val(  $( "#slider-range-min" ).slider( "value" ) );
              $( "#amounts" ).val(  $( "#slider-range-min" ).slider( "value" )*youhui);
            });
        </script>
